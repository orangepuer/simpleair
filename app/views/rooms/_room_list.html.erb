<% @rooms.each do |room| %>
  <div class="col-md-4">
    <div class="panel panel-default">
      <div class="panel-heading preview">
        <%= image_tag room.photos.first.variant(resize: "300x300") %>
      </div>
      <div class="panel-body">
        <%= link_to room.listing_name, room %>
      </div>
    </div>
  </div>
<% end %>